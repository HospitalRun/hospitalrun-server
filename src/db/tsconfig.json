// we need this custom tsconfig.json to handle CouchDB Javascript Context locally to this folder
{
  "extends": "../../tsconfig.json",
  "include": [
    "."
  ],
  "exclude": [
    "node_modules",
  ],
  "compilerOptions": {
    "typeRoots": [
      "../../node_modules/@types",
      "./context.d.ts"
    ],
    "outDir": "../../dist/db",
  }
}
